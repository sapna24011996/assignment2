<div class="container">
    <h3><a href="/pages/contacts">Contact List</a></h3>
    <h1>Edit Contact</h1>
    <form action="/pages/contacts/update/<%= contact._id%>" method="post">
      <table>
        <tbody>
          <tr>
            <td>Name</td>
            <td><input type="text" name="name" value="<%= contact.name %>" /></td>
          </tr>
          <tr>
            <td>Mobile</td>
            <td><input type="text" name="mobile" value="<%= contact.mobile %>"</textarea></td>
          </tr>
          <tr>
            <td>Email</td>
            <td><input type="text" name="email" value="<%= contact.email %>" /></td>
          </tr>
          <tr>
            <td colspan="2"><button type="submit">Update</button></td>
          </tr>
        </tbody>
      </table>
    </form>
  </div>